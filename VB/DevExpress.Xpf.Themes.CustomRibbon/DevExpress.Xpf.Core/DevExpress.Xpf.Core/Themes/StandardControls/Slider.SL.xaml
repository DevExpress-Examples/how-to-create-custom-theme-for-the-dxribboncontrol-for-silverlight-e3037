    <Style x:Key="SliderRepeatButtonStyle" TargetType="RepeatButton">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="IsTabStop" Value="False" />
        
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="RepeatButton">
                    <Border Background="{TemplateBinding Background}" />
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="SliderThumbStyle" TargetType="Thumb">
        <Setter Property="Foreground" Value="{x:Null}" />
        <Setter Property="Background" Value="{x:Null}" />
        <Setter Property="Height" Value="22" />
        <Setter Property="Width" Value="12" />
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Thumb">
                    <Grid>
                        <Border x:Name="Shadow" Background="#FF000000" CornerRadius="2" Margin="1,1,-1,-1" Opacity="0.10" />
                        <Grid x:Name="DefaultState">
                            <Border Background="{StaticResource SliderThumb.DefaultState.Border1.Background}" CornerRadius="2" Padding="1">
                                <Border Background="{StaticResource SliderThumb.DefaultState.Border2.Background}" CornerRadius="1" Padding="1">
                                    <Border Background="{StaticResource SliderThumb.DefaultState.Border3.Background}" CornerRadius="0" />
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="HoverState" Opacity="0">
                            <Border Background="{StaticResource SliderThumb.HoverState.Border1.Background}" CornerRadius="2" Padding="1">
                                <Border Background="{StaticResource SliderThumb.HoverState.Border2.Background}" CornerRadius="1" Padding="1">
                                    <Border Background="{StaticResource SliderThumb.HoverState.Border3.Background}" CornerRadius="0" />
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="PressedState" Opacity="0">
                            <Border Background="{StaticResource SliderThumb.PressedState.Border1.Background}" CornerRadius="2" Padding="1">
                                <Border Background="{StaticResource SliderThumb.PressedState.Border2.Background}" CornerRadius="1" Padding="1">
                                    <Border Background="{StaticResource SliderThumb.PressedState.Border3.Background}" CornerRadius="0" />
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="DisabledState" Opacity="0">
                            <Border Background="{StaticResource SliderThumb.DisabledState.Border1.Background}" CornerRadius="2" Padding="1">
                                <Border Background="{StaticResource SliderThumb.DisabledState.Border2.Background}" CornerRadius="1" Padding="1">
                                    <Border Background="{StaticResource SliderThumb.DisabledState.Border3.Background}" CornerRadius="0" />
                                </Border>
                            </Border>
                        </Grid>
                    </Grid>
                    
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

