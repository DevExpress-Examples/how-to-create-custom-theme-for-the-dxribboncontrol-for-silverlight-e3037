                                    <TextBlock Text="{TemplateBinding ReplaceLabelText}" Style="{StaticResource SearchPanelThemeKey_PanelTextBlockStyle}" UseLayoutRounding="True" />

                                    <dxe:ButtonEdit x:Name="tbReplaceString" Width="150" FontSize="13" Margin="0,0,8,0" IsTextEditable="True" EditValue="{Binding ReplaceString, Mode=TwoWay}" AllowNullInput="False" AllowDefaultButton="False" UseLayoutRounding="True" />
