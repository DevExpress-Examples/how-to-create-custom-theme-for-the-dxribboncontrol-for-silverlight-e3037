                                    <ContentPresenter ContentTemplate="{StaticResource SearchPanelThemeKey_OptionsButtonShape}" UseLayoutRounding="True" />
                                </Button>
                            </StackPanel>

                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right" UseLayoutRounding="True">
                                <StackPanel x:Name="ReplaceButtons" Orientation="Horizontal" Visibility="{Binding ReplaceMode, Converter={StaticResource boolToVisibilityConverter}}" UseLayoutRounding="True">

                                    <Button x:Name="ReplaceButton" Style="{StaticResource SearchPanelThemeKey_ReplaceButtonStyle}" CommandParameter="{Binding Path=CommandParameter, RelativeSource={RelativeSource TemplatedParent}}" Command="{Binding ReplaceForwardCommand}" UseLayoutRounding="True">
                                        <TextBlock Text="{TemplateBinding ReplaceButtonText}" Style="{StaticResource SearchPanelThemeKey_ButtonTextBlockStyle}" UseLayoutRounding="True" />
