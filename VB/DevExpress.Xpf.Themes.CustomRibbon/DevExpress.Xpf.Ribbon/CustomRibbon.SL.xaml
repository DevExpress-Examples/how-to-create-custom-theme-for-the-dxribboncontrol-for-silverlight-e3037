                        <GradientStop Color="#FFFFFFFF" Offset="0.5" />
                    </LinearGradientBrush>
                </Border.OpacityMask>
                <Path Fill="#FF000000" Stretch="Fill" Data="F1M1323,792L1327,788 1331,792 1323,792z" Margin="2" Width="8" Height="4" HorizontalAlignment="Center" VerticalAlignment="Center" />
            </Border>
            <Border Background="#14FFFFFF" Height="1" VerticalAlignment="Top" />
