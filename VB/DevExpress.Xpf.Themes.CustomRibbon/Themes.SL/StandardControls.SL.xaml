                                            </Border>
                                        </Grid>
                                        <Grid x:Name="RadioButtonIcon_PressedState" Opacity="0">
                                            <Border Background="{StaticResource RadioButton.PressedState.Icon.Border1}" CornerRadius="64">
                                                <Border Background="{StaticResource RadioButton.PressedState.Icon.Border2}" Margin="1" CornerRadius="64" />
