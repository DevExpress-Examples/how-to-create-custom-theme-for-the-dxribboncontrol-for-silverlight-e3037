                            </ToolTipService.ToolTip>
                            <Ellipse Stroke="#FFDB000C" StrokeThickness="1" Width="14" Height="14" />
                            <Ellipse Fill="#FFDB000C" Margin="0,-2,-1,0" Width="4" Height="4" HorizontalAlignment="Right" VerticalAlignment="Top" />
