	<!-- Blue: Ribbon Quick Access Toolbar Button Template -->
	<ControlTemplate x:Key="QuickAccessToolbarThemeKey_RibbonToolbarCustomizationButtonTemplate" TargetType="dxr:RibbonCheckedBorderControl">
		<Grid UseLayoutRounding="True">
            <Grid x:Name="PART_Normal" Opacity="0">
                <Border UseLayoutRounding="True" BorderThickness="0" CornerRadius="2" Background="Transparent">
                    <Border UseLayoutRounding="True" BorderThickness="0" CornerRadius="1" Margin="1" Background="Transparent">
                        <Border UseLayoutRounding="True" BorderThickness="0" CornerRadius="0" Margin="1" Background="Transparent" />
                    </Border>
                </Border>
            </Grid>
            <Grid x:Name="PART_Hover" Visibility="Collapsed">
				<Border UseLayoutRounding="True" x:Name="hover" BorderThickness="0" CornerRadius="2">
					<Border.Background>
						<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
							<GradientStop Color="#FFDDCF9B" Offset="0" />
							<GradientStop Color="#FFDDCF9B" Offset="1" />
							<GradientStop Color="#FFC0A776" Offset="0.5" />
						</LinearGradientBrush>
					</Border.Background>
					<Border UseLayoutRounding="True" x:Name="hover1" BorderThickness="0" CornerRadius="1" Margin="1">
						<Border.Background>
							<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
								<GradientStop Color="White" Offset="0" />
								<GradientStop Color="#FFFFF362" Offset="1" />
							</LinearGradientBrush>
						</Border.Background>
						<Border UseLayoutRounding="True" x:Name="hover2" BorderThickness="0" CornerRadius="0" Margin="1">
							<Border.Background>
								<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
									<GradientStop Color="#FFFFFDDE" Offset="0" />
									<GradientStop Color="#FFFFE794" Offset="1" />
									<GradientStop Color="#FFFFE797" Offset="0.5" />
									<GradientStop Color="#FFFFD758" Offset="0.5" />
								</LinearGradientBrush>
							</Border.Background>
						</Border>
					</Border>
				</Border>
			</Grid>
			<Grid x:Name="PART_Pressed" Visibility="Collapsed">
				<Border UseLayoutRounding="True" x:Name="pressed" BorderThickness="0" CornerRadius="2">
					<Border.Background>
						<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
							<GradientStop Color="#FF8B7654" Offset="0" />
							<GradientStop Color="#FFC4B9A8" Offset="1" />
						</LinearGradientBrush>
					</Border.Background>
					<Grid>
						<Grid.RowDefinitions>
							<RowDefinition Height="0.1*" />
							<RowDefinition Height="*" />
						</Grid.RowDefinitions>
						<Border UseLayoutRounding="True" x:Name="pressed1" Grid.RowSpan="2" BorderThickness="0" CornerRadius="1,1,0,0" Margin="1,1,1,0">
							<Border.Background>
								<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
									<GradientStop Color="#FFFDAD11" Offset="1" />
									<GradientStop Color="#FFF0A85C" />
								</LinearGradientBrush>
							</Border.Background>
							<Border UseLayoutRounding="True" x:Name="pressed2" BorderThickness="0" CornerRadius="0" Margin="1">
								<Border.Background>
									<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
										<GradientStop Color="#FFFFC570" Offset="1" />
										<GradientStop Color="#FFFCA160" Offset="0.5" />
										<GradientStop Color="#FFFB8C3B" Offset="0.5" />
										<GradientStop Color="#FFF8B56A" />
									</LinearGradientBrush>
								</Border.Background>
							</Border>
						</Border>
						<Border UseLayoutRounding="True" x:Name="pressed3" Margin="0" Opacity="0.5" BorderBrush="{x:Null}" BorderThickness="0" CornerRadius="2,2,0,0">
							<Border.Background>
								<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
									<GradientStop Color="#FF352D21" Offset="0.1" />
									<GradientStop Color="#00352D21" Offset="1" />
								</LinearGradientBrush>
							</Border.Background>
						</Border>
					</Grid>
				</Border>
			</Grid>
            <Grid x:Name="PART_Focused" Visibility="Collapsed">
                <Border UseLayoutRounding="True" BorderThickness="0" CornerRadius="2">
                    <Border.Background>
                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                            <GradientStop Color="#FFDDCF9B" Offset="0" />
                            <GradientStop Color="#FFDDCF9B" Offset="1" />
                            <GradientStop Color="#FFC0A776" Offset="0.5" />
                        </LinearGradientBrush>
                    </Border.Background>
                    <Border UseLayoutRounding="True" BorderThickness="0" CornerRadius="1" Margin="1">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="White" Offset="0" />
                                <GradientStop Color="#FFFFF362" Offset="1" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border UseLayoutRounding="True" BorderThickness="0" CornerRadius="0" Margin="1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FFFFFDDE" Offset="0" />
                                    <GradientStop Color="#FFFFE794" Offset="1" />
                                    <GradientStop Color="#FFFFE797" Offset="0.5" />
                                    <GradientStop Color="#FFFFD758" Offset="0.5" />
                                </LinearGradientBrush>
                            </Border.Background>
                        </Border>
                    </Border>
                </Border>
            </Grid>
            <Grid>
				<Grid UseLayoutRounding="True" Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center">
					<StackPanel UseLayoutRounding="True" HorizontalAlignment="Center" VerticalAlignment="Center" Width="5" Orientation="Vertical" Margin="4,0,4,1">
						<Grid UseLayoutRounding="True" Width="6" Height="4">
							<Rectangle UseLayoutRounding="True" Stretch="Fill" Fill="#ffcdcdcd" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Bottom" Width="6" Height="1" />
							<Rectangle UseLayoutRounding="True" Stretch="Fill" Fill="#FF567db1" HorizontalAlignment="Left" Margin="0,0,0,1" VerticalAlignment="Bottom" Width="6" Height="1" />
						</Grid>
						<Grid UseLayoutRounding="True" Margin="0,1,0,0" Width="Auto" Height="4">
							<Path UseLayoutRounding="True" Width="Auto" Height="Auto" Stretch="Fill" Fill="#ffcdcdcd" Data="F1 M 3.05176e-005,1L 3.05176e-005,2L 1,2.00003L 1,3.00003L 2,3.00003L 2,4.00003L 3,4.00003L 3,3.00003L 4,3.00003L 4,2.00003L 5,2.00003L 5,1.00003L 3.05176e-005,1 Z " Margin="0,1,0,0" />
							<Path UseLayoutRounding="True" Width="Auto" Height="Auto" Stretch="Fill" Fill="#FF567db1" Data="F1 M 0,0L 0,1L 0.999969,1L 0.999969,2L 1.99997,2L 1.99997,3L 2.99997,3L 2.99997,2L 3.99997,2L 3.99997,1L 4.99997,1L 4.99997,0L 0,0 Z " Margin="0,0,0,1" />
						</Grid>
					</StackPanel>
				</Grid>
			</Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Hover">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Visible</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Pressed" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Focused" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="PART_Hover" Storyboard.TargetProperty="Visibility">
                                <DiscreteObjectKeyFrame KeyTime="0:0:0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <Visibility>Collapsed</Visibility>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
