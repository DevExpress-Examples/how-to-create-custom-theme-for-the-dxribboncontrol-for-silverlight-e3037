			</Border>
		</Border>
	</ControlTemplate>
	<ControlTemplate x:Key="RibbonThemeKey_ApplicationMenuBottomPaneTemplate" TargetType="ContentControl">
		<Grid>
			<Border UseLayoutRounding="True" BorderThickness="0,1,0,0" BorderBrush="#FF9BAFCA">
				<Border UseLayoutRounding="True" BorderThickness="0,1,0,0" Margin="-1,0,-1,0" BorderBrush="#FFFFFFFF">
					<Border UseLayoutRounding="True" Margin="-3,0,-3,-3">
