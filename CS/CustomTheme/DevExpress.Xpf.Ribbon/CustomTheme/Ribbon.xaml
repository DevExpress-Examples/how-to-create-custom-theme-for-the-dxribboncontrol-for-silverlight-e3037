	<!-- office2007Blue: Application Menu Border -->
	<ControlTemplate x:Key="RibbonThemeKey_ApplicationMenuBorderTemplate" TargetType="ContentControl">
		<Grid x:Name="PART_AppMenu" Margin="0,0,3,3">
			<Grid.RowDefinitions>
				<RowDefinition Height="23" />
				<RowDefinition Height="*" />
			</Grid.RowDefinitions>
			<Border x:Name="Shadow" Margin="3,3,-3,-3" UseLayoutRounding="True" CornerRadius="3" Background="#FF000000" Opacity="0.1" Grid.RowSpan="2" />
			<Border x:Name="border1" Background="#FF9bafca" CornerRadius="3" Margin="0" Grid.RowSpan="2" />
			<Border x:Name="border2" Background="#FFFFFFFF" CornerRadius="2" Margin="1" Grid.RowSpan="2" />
			<Border x:Name="border3" CornerRadius="1,1,0,0" UseLayoutRounding="True" Margin="2,2,2,0">
				<Border.Background>
					<LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
						<GradientStop Color="#FFd7e5f7" Offset="0" />
						<GradientStop Color="#FFb7cae6" Offset="1" />
						<GradientStop Color="#FFd4e1f3" Offset="0.5" />
						<GradientStop Color="#FFc6d8ee" Offset="0.5" />
					</LinearGradientBrush>
				</Border.Background>
			</Border>
			<Border x:Name="border4" BorderThickness="0" CornerRadius="0,0,2,2" UseLayoutRounding="True" Margin="1,0,1,1" Grid.Row="1" Background="White">
				<Border x:Name="border5" BorderThickness="0" Background="#FFBFD3EB" CornerRadius="0,0,1,1" Margin="1,0,1,1" Grid.Row="1">
					<Border Margin="2,1" BorderThickness="1" BorderBrush="White">
						<Border Margin="0" Background="#FF9BAFCA">
							<Border Margin="1" Background="#FFFAFAFA">
								<ContentPresenter />
							</Border>
						</Border>
					</Border>
				</Border>
			</Border>
		</Grid>
		
	</ControlTemplate>
	<ControlTemplate x:Key="RibbonThemeKey_ApplicationMenuContentControlBorderTemplate" TargetType="ContentControl">
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Height="Auto" />
				<RowDefinition Height="*" />
			</Grid.RowDefinitions>
			<Border Grid.Row="0" Background="#FF1F48A1" Height="3">
				<Border Background="#FF589BF1" Margin="0,1,0,0" BorderThickness="0,0,0,1" BorderBrush="#FF467EDC" />
			</Border>
			<Border Background="White" BorderThickness="0" Margin="0" Grid.Row="1">
				<ContentPresenter Content="{TemplateBinding Content}" />
			</Border>
		</Grid>
	</ControlTemplate>
	<ControlTemplate x:Key="RibbonThemeKey_ApplicationMenuRightPaneTemplate" TargetType="ContentControl">
		<Border Background="#fffefefe">
