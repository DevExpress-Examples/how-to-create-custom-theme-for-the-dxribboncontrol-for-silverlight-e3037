								<GradientStop Color="#FFb2caea" Offset="0.5" />
							</LinearGradientBrush>
						</Border.Background>
						<ContentPresenter UseLayoutRounding="True" HorizontalAlignment="Right" Margin="2" />
					</Border>
				</Border>
			</Border>
		</Grid>
	</ControlTemplate>
