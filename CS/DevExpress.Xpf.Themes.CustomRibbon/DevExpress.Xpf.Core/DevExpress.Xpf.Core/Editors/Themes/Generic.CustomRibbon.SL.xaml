                                        <Binding Source="{x:Null}" ConverterParameter="FilterPanelEditFilter" Converter="{StaticResource EditorStringIdConverter}" />
                                    </ToolTipService.ToolTip>
                                    <Button.Visibility>
                                        <Binding Path="AllowFilterEditor" RelativeSource="{RelativeSource TemplatedParent}">
                                            <Binding.Converter>
                                                <dxc:BooleanToVisibilityConverter />
                                            </Binding.Converter>
                                        </Binding>
                                    </Button.Visibility>
                                </Button>
                                <dxepc:CheckEdit Style="{StaticResource FilterPanelControlBaseThemeKey_FilterPanelEnableFilterStyle}" x:Name="PART_FilterPanelIsActiveButton" VerticalAlignment="Center" dxc:DXDockPanel.Dock="Left" EditValue="{Binding IsFilterEnabled, RelativeSource={RelativeSource TemplatedParent}, Mode=TwoWay}" IsEnabled="{Binding IsCanEnableFilter, RelativeSource={RelativeSource TemplatedParent}}">
                                </dxepc:CheckEdit>
                                <dxepc:ComboBoxEdit x:Name="PART_FilterPanelMRUComboBox" VerticalAlignment="Center" IsTextEditable="False" dxc:DXDockPanel.Dock="Left" HorizontalAlignment="Left" ShowEditorButtons="False" ShowBorder="False" EditMode="InplaceActive" ItemsSource="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=MRUFilters}" AllowDefaultButton="False" EditNonEditableTemplate="{StaticResource FilterPanelControlBaseThemeKey_MRUComboBoxTemplate}" DisplayTemplate="{StaticResource FilterPanelControlBaseThemeKey_MRUComboBoxTemplate}" />
