        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="dxef:FilterPanelControlBase">
                        <Border BorderThickness="0,1,0,0" Style="{StaticResource FilterPanelControlBaseThemeKey_FilterPanelBorderStyle}">
                            <dxc:DXDockPanel>
                                <Button Name="PART_FilterPanelCloseButton" Command="{Binding ClearFilterCommand, RelativeSource={RelativeSource TemplatedParent}}" dxc:DXDockPanel.Dock="Right" VerticalAlignment="Center" Margin="0,0,6,0" Template="{StaticResource FilterPanelControlBaseThemeKey_ClearFilterButtonTemplate}">
                                    <ToolTipService.ToolTip>
                                        <Binding Source="{x:Null}" ConverterParameter="FilterPanelClearFilter" Converter="{StaticResource EditorStringIdConverter}" />
                                    </ToolTipService.ToolTip>
                                </Button>
                                <Button Name="PART_FilterControlButton" Command="{Binding ShowFilterEditorCommand, RelativeSource={RelativeSource TemplatedParent}}" dxc:DXDockPanel.Dock="Right" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,6,0" Template="{StaticResource FilterPanelControlBaseThemeKey_FilterControlButtonTemplate}">
