    <internal:TextBoxPaddingConverter x:Key="TextEditTextBoxPaddingConverter" />
    <SolidColorBrush x:Key="TextEditThemeKey_NullTextForeground" Color="#6D6D6D" />

    <internal:HasFocusToVisualStateNameConverter x:Key="HasFocusToVisualStateNameConverter" />


