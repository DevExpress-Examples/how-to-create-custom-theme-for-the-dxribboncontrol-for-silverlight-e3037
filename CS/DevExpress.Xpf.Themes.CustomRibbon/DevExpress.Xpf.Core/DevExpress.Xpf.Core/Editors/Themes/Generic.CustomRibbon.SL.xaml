        <Setter Property="UseLayoutRounding" Value="True" />


        <Setter Property="dx:DXStyleHelper.BindableSetters">
            <Setter.Value>
                <dxc:DXBindableSetterCollection>
                    <dxc:DXBindableSetter Property="TextAlignment" Path="HorizontalContentAlignment" Converter="{StaticResource HorizontalContentAlignmentToTextAlignmentConverter}" />
                    <dxc:DXBindableSetter Property="VerticalAlignment" Path="VerticalContentAlignment" />
                    <dxc:DXBindableSetter Property="TextWrapping" Path="TextWrapping" />
                    <dxc:DXBindableSetter Property="TextDecorations" Path="TextDecorations" />
                    <dxc:DXBindableSetter Property="Padding" Path="Padding" />
                </dxc:DXBindableSetterCollection>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="BaseTextBlockInplaceStyle" TargetType="TextBlock">
