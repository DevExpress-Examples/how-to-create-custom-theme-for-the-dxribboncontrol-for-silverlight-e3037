    <ControlTemplate x:Key="ColorChooserThemeKey_ControlTemplate" TargetType="dxe:ColorChooser">
        <Border Background="{TemplateBinding Background}" UseLayoutRounding="True" Height="289">
            <Grid x:Name="colorChooser" Margin="12">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto" />
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>
                <Grid x:Name="left_part">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="Auto" />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*" />
                        <ColumnDefinition Width="Auto" />
                    </Grid.ColumnDefinitions>
                    <Border x:Name="XY" Background="#FFF7C6C6" BorderThickness="1" BorderBrush="#FFB1B1B1">
                        <Canvas x:Name="PART_xyColorArea" Width="289" Height="240">
                            <Rectangle Fill="#FFFFFFFF" Width="289" Height="240">
                            </Rectangle>
                            <Rectangle x:Name="PART_xColorAreaRect" RenderTransformOrigin="0.5,0.412" Width="240" Height="289">
                                <Rectangle.RenderTransform>
                                    <TransformGroup>
                                        <RotateTransform Angle="-90" />
                                    </TransformGroup>
                                </Rectangle.RenderTransform>
                                <Rectangle.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop x:Name="xColorStart" Color="#00000000" Offset="0" />
                                        <GradientStop x:Name="xColorEnd" Color="#FFFF0000" Offset="1" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Rectangle x:Name="PART_yColorAreaRect" RenderTransformOrigin="0.5,0.5" Width="289" Height="240">
                                <Rectangle.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop x:Name="yColorStart" Color="#00000000" Offset="0" />
                                        <GradientStop x:Name="yColorEnd" Color="#FF000000" Offset="1" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Canvas Width="21" Height="21" Canvas.Left="{Binding Path=ActualXYThumbXOffset, RelativeSource={RelativeSource TemplatedParent}}" Canvas.Top="{Binding Path=ActualXYThumbYOffset, RelativeSource={RelativeSource TemplatedParent}}">
                                <Rectangle Width="21" Height="21" Fill="Transparent" />

                                <Path Width="9" Height="1" Fill="#FF000000" Stretch="Fill" Stroke="#FFFFFFFF" Data="M0,9 L8,9" Canvas.Left="0" Canvas.Top="9" />
                                <Path Width="9" Height="1" Fill="#FF000000" Stretch="Fill" Stroke="#FF000000" Data="M0,10 L8,10" Canvas.Left="0" Canvas.Top="10" />
                                <Path Width="9" Height="1" Fill="#FF000000" Stretch="Fill" Stroke="#FFFFFFFF" Data="M0,11 L8,11" Canvas.Left="0" Canvas.Top="11" />

                                <Path Width="9" Height="1" Fill="#FF000000" Stretch="Fill" Stroke="#FFFFFFFF" Data="M0,9 L8,9" Canvas.Left="12" Canvas.Top="9" />
                                <Path Width="9" Height="1" Fill="#FF000000" Stretch="Fill" Stroke="#FF000000" Data="M0,10 L8,10" Canvas.Left="12" Canvas.Top="10" />
                                <Path Width="9" Height="1" Fill="#FF000000" Stretch="Fill" Stroke="#FFFFFFFF" Data="M0,11 L8,11" Canvas.Left="12" Canvas.Top="11" />

                                <Path Width="1" Height="9" Fill="#FF000000" Stretch="Fill" Stroke="#FFFFFFFF" Data="M9,0 L9,8" Canvas.Left="9" Canvas.Top="0" />
                                <Path Width="1" Height="9" Fill="#FF000000" Stretch="Fill" Stroke="#FF000000" Data="M10,0 L10,8" Canvas.Left="10" Canvas.Top="0" />
                                <Path Width="1" Height="9" Fill="#FF000000" Stretch="Fill" Stroke="#FFFFFFFF" Data="M11,0 L11,8" Canvas.Left="11" Canvas.Top="0" />

                                <Path Width="1" Height="9" Fill="#FF000000" Stretch="Fill" Stroke="#FFFFFFFF" Data="M9,0 L9,8" Canvas.Left="9" Canvas.Top="12" />
                                <Path Width="1" Height="9" Fill="#FF000000" Stretch="Fill" Stroke="#FF000000" Data="M10,0 L10,8" Canvas.Left="10" Canvas.Top="12" />
                                <Path Width="1" Height="9" Fill="#FF000000" Stretch="Fill" Stroke="#FFFFFFFF" Data="M11,0 L11,8" Canvas.Left="11" Canvas.Top="12" />
                            </Canvas>
                        </Canvas>
                    </Border>
                    <Border x:Name="X" Grid.Row="1" Background="#FFBEEFB3" Margin="0,4,0,0" BorderThickness="1" BorderBrush="#FFB1B1B1">
                        <Canvas x:Name="PART_aColorArea" Width="289" Height="16">
                            <Rectangle Width="289" Height="16" Fill="#FFD3D3D3">
                            </Rectangle>
                            <Rectangle Width="289" Height="16">
                                <Rectangle.Fill>


                                    <SolidColorBrush Color="Gray" />

                                </Rectangle.Fill>
                            </Rectangle>
                            <Rectangle Width="16" Height="289" Canvas.Top="16">
                                <Rectangle.Fill>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop x:Name="aColorStart" Color="#00FFFFFF" Offset="0" />
                                        <GradientStop x:Name="aColorEnd" Color="#FFFFFFFF" Offset="1" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                                <Rectangle.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="1" ScaleY="1" />
                                        <SkewTransform AngleX="0" AngleY="0" />
                                        <RotateTransform Angle="-90" />
                                        <TranslateTransform X="0" Y="0" />
                                    </TransformGroup>
                                </Rectangle.RenderTransform>
                            </Rectangle>
                            <Path x:Name="PART_aThumbUp" Width="10" Height="5" Data="M0,0 L5,5 10,0 z" Fill="#FF000000" Canvas.Left="{Binding Path=ActualAThumbOffset, RelativeSource={RelativeSource TemplatedParent}}" IsHitTestVisible="False" />
                            <Path x:Name="PART_aThumbDown" Width="10" Height="5" Data="M 0,5 L5,0 10,5 z" Fill="#FF000000" Canvas.Top="11" Canvas.Left="{Binding Path=ActualAThumbOffset, RelativeSource={RelativeSource TemplatedParent}}" IsHitTestVisible="False" />
                        </Canvas>
                    </Border>
                    <Border x:Name="Y" Background="#FFCAD7FB" Grid.Column="1" Margin="4,0,0,0" BorderThickness="1" BorderBrush="#FFB1B1B1">
                        <Canvas x:Name="PART_resColorArea" Height="240" Width="16">
                            <Rectangle x:Name="PART_resColorAreaRect" Height="240" Width="16" IsHitTestVisible="True">
                                <Rectangle.Fill>
                                    <LinearGradientBrush x:Name="zColor" EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop x:Name="zColor1" Color="#FFFF0000" Offset="0" />
                                        <GradientStop x:Name="zColor2" Color="#FFFFFF00" Offset="0.166666" />
                                        <GradientStop x:Name="zColor3" Color="#FF00FF00" Offset="0.333333" />
                                        <GradientStop x:Name="zColor4" Color="#FF00FFFF" Offset="0.5" />
                                        <GradientStop x:Name="zColor5" Color="#FF0000FF" Offset="0.666666" />
                                        <GradientStop x:Name="zColor6" Color="#FFFF00FF" Offset="0.833333" />
                                        <GradientStop x:Name="zColor7" Color="#FFFF0000" Offset="1" />
                                    </LinearGradientBrush>
                                </Rectangle.Fill>
                            </Rectangle>
                            <Path x:Name="PART_zThumbLeft" Width="5" Height="10" Data="M0,0 L5,5 0,10 z" Fill="#FF000000" Canvas.Top="{Binding Path=ActualZThumbOffset, RelativeSource={RelativeSource TemplatedParent}}" IsHitTestVisible="False" />
                            <Path x:Name="PART_zThumbRight" Width="5" Height="10" Data="M 5,0 L0,5 5,10 z" Fill="#FF000000" Canvas.Left="11" Canvas.Top="{Binding Path=ActualZThumbOffset, RelativeSource={RelativeSource TemplatedParent}}" IsHitTestVisible="False" />
                        </Canvas>
                    </Border>
                </Grid>
                <Grid x:Name="right" Grid.Column="1" HorizontalAlignment="Left" Margin="16,0,0,0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto" />
                        <RowDefinition Height="*" />
                    </Grid.RowDefinitions>
                    <StackPanel x:Name="RGBA">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock VerticalAlignment="Center" MinWidth="16"><Run Text="R:" /></TextBlock>
                            <dxepc:TextEdit MaskType="RegEx" Mask="([01]?\d\d?|2[0-4]\d|25[0-5])" x:Name="PART_rChannel" Grid.Column="1" />
                        </Grid>
                        <Grid Margin="0,4,0,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock VerticalAlignment="Center" MinWidth="16"><Run Text="G:" /></TextBlock>
                            <dxepc:TextEdit MaskType="RegEx" Mask="([01]?\d\d?|2[0-4]\d|25[0-5])" x:Name="PART_gChannel" Grid.Column="1" />
                        </Grid>
                        <Grid Margin="0,4,0,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock VerticalAlignment="Center" MinWidth="16"><Run Text="B:" /></TextBlock>
                            <dxepc:TextEdit MaskType="RegEx" Mask="([01]?\d\d?|2[0-4]\d|25[0-5])" x:Name="PART_bChannel" Grid.Column="1" />
                        </Grid>
                        <Grid Margin="0,4,0,0">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto" />
                                <ColumnDefinition Width="*" />
                            </Grid.ColumnDefinitions>
                            <TextBlock VerticalAlignment="Center" MinWidth="16"><Run Text="A:" /></TextBlock>
                            <dxepc:TextEdit MaskType="RegEx" Mask="([01]?\d\d?|2[0-4]\d|25[0-5])" x:Name="PART_aChannel" Grid.Column="1" />
                        </Grid>
                    </StackPanel>
                    <StackPanel x:Name="currentColor" Margin="0,16,0,0" Grid.Row="1" VerticalAlignment="Bottom">
                        <Border Height="75" Background="#FFB1B1B1" CornerRadius="4">
                            <Border Margin="1" Background="White" CornerRadius="3">
                                <Border Margin="2" CornerRadius="2">
                                    <Border.Background>
                                        <SolidColorBrush Color="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}}" />
                                    </Border.Background>
                                </Border>
                            </Border>
                        </Border>
                        <dxepc:TextEdit x:Name="PART_ResultColor" EditValue="{Binding Path=Color, RelativeSource={RelativeSource TemplatedParent}}" IsReadOnly="True" Margin="0,4,0,0" Width="80" />
                    </StackPanel>
                </Grid>
            </Grid>
        </Border>
    </ControlTemplate>

