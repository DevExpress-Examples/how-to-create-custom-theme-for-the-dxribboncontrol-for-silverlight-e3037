    <ControlTemplate x:Key="TabControlHeaderMenuThemeKey_TabControlHeaderMenuItemTemplate" TargetType="dx:TabControlHeaderMenuItem">
        <Grid>
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal" />
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="HoverState" Storyboard.TargetProperty="Opacity">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <sys:Double>1</sys:Double>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="SelectionStates">
                    <VisualState x:Name="Selected">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Check" Storyboard.TargetProperty="Opacity">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <sys:Double>1</sys:Double>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Icon" Storyboard.TargetProperty="Opacity">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <sys:Double>0</sys:Double>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Unselected">
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Check" Storyboard.TargetProperty="Opacity">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <sys:Double>0</sys:Double>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="Icon" Storyboard.TargetProperty="Opacity">
                                <DiscreteObjectKeyFrame KeyTime="0">
                                    <DiscreteObjectKeyFrame.Value>
                                        <sys:Double>1</sys:Double>
                                    </DiscreteObjectKeyFrame.Value>
                                </DiscreteObjectKeyFrame>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border Padding="7,3,8,2">
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto" />
                        <ColumnDefinition Width="*" />
                    </Grid.ColumnDefinitions>
                    <Border Background="#FFE9EEEE" BorderBrush="#FFC5C5C5" BorderThickness="0,0,1,0" Margin="-7,-3,0,-2" CornerRadius="2,0,0,2" Grid.Column="0" />
                    <Border x:Name="HoverState" Margin="-6,-2,-7,-1" CornerRadius="2,2,2,2" Padding="1" Grid.ColumnSpan="2" Opacity="0">
                        <Border.Background>
                            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                <GradientStop Color="#FFDBCE99" Offset="0" />
                                <GradientStop Color="#FFBBA67F" Offset="1" />
                                <GradientStop Color="#FFB9A074" Offset="0.5" />
                            </LinearGradientBrush>
                        </Border.Background>
                        <Border CornerRadius="1,1,1,1" Padding="1,1,1,1">
                            <Border.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FFFFFFFD" Offset="0" />
                                    <GradientStop Color="#FFFFF9E0" Offset="1" />
                                    <GradientStop Color="#FFFFF1BA" Offset="0.5" />
                                </LinearGradientBrush>
                            </Border.Background>
                            <Border CornerRadius="1,1,1,1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFFFFDF2" Offset="0" />
                                        <GradientStop Color="#FFFFEEAE" Offset="1" />
                                        <GradientStop Color="#FFFFEA9A" Offset="0.5" />
                                        <GradientStop Color="#FFFFDC58" Offset="0.5" />
                                    </LinearGradientBrush>
                                </Border.Background>
                            </Border>
                        </Border>
                    </Border>
                    <dx:DXContentPresenter x:Name="Icon" Content="{TemplateBinding Icon}" HorizontalAlignment="Center" VerticalAlignment="Center" Width="16" Height="16" Margin="0,0,4,0" Grid.Column="0" />
                    <dx:DXContentPresenter x:Name="Check" HorizontalAlignment="Center" VerticalAlignment="Center" Margin="0,0,4,0" Grid.Column="0" FlowDirection="LeftToRight" Opacity="0">
                        <dx:DXContentPresenter.Content>
                            <Border x:Name="ElementCheck" Background="#FFF29536" Width="20" Height="20" CornerRadius="2" Padding="1">
                                <Border Background="#FFFFE395" CornerRadius="1">
                                    <Path Stretch="None" Fill="#FF30337A" Data="F1M1,5L4,9 9,0 10,1 4,12 0,6 1,5z" Width="10" Height="12" />
                                </Border>
                            </Border>
                        </dx:DXContentPresenter.Content>
                    </dx:DXContentPresenter>
                    <dx:DXContentPresenter Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Margin="4,0,0,0" HorizontalAlignment="Left" VerticalAlignment="Center" Grid.Column="1" />
                </Grid>
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="TabControlHeaderMenuThemeKey_TabControlHeaderMenuSeparatorTemplate" TargetType="dx:TabControlHeaderMenuSeparator">
