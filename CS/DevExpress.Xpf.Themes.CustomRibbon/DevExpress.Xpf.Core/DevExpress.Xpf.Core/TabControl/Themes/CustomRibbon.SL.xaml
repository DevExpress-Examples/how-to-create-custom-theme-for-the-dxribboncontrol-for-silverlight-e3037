                                <DiscreteObjectKeyFrame KeyTime="0" Value="*" />
                                
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <dx:HeaderContainerPresenter x:Name="headerContainerPresenter">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition x:Name="rowFoneUp" Height="*" />
                        <RowDefinition x:Name="rowMain" Height="*" />
                        <RowDefinition x:Name="rowFoneBottom" Height="*" />
                    </Grid.RowDefinitions>
                    <Border x:Name="foneUpNormal" Background="#FFF4F8FF" BorderBrush="#FF898C95" BorderThickness="1,0,1,0" Margin="-1,1,0,0" Grid.Row="0" IsHitTestVisible="False" Opacity="1" />
                    <Border x:Name="foneUpSelected" Background="#FFFFFFFF" BorderBrush="#FF898C95" BorderThickness="1,0,1,0" Margin="-1,1,0,0" Grid.Row="0" IsHitTestVisible="False" Opacity="0" />
                    <Border x:Name="foneBottomNormal" Background="#FFF4F8FF" BorderBrush="#FF898C95" BorderThickness="1,0,1,0" Margin="-1,0,0,1" Grid.Row="2" IsHitTestVisible="False" Opacity="1" />
                    <Border x:Name="foneBottomSelected" Background="#FFFFFFFF" BorderBrush="#FF898C95" BorderThickness="1,0,1,0" Margin="-1,0,0,1" Grid.Row="2" IsHitTestVisible="False" Opacity="0" />
                    <Grid Grid.Row="1">
                        <Grid x:Name="borderContainer">
                            <Border x:Name="normalBorder" BorderBrush="#FF898C95" BorderThickness="1,1,1,0" Margin="-1,0,0,0" Padding="1,1,1,0" Opacity="1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFFCFCFC" Offset="0" />
                                        <GradientStop Color="#FFF3F3F3" Offset="1" />
                                        <GradientStop Color="#FFFAFAFA" Offset="0.5" />
                                        <GradientStop Color="#FFF6F6F6" Offset="0.5" />
                                    </LinearGradientBrush>
                                </Border.Background>
                                <Border x:Name="normal">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFF2F2F2" Offset="0" />
                                            <GradientStop Color="#FFCFCFCF" Offset="1" />
                                            <GradientStop Color="#FFEBEBEB" Offset="0.5" />
                                            <GradientStop Color="#FFDDDDDD" Offset="0.5" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                            <Border x:Name="selected" Background="#FFFFFFFF" BorderBrush="#FF898C95" BorderThickness="1,1,1,0" Margin="-1,-2,0,-1" Opacity="0">
                                <Border x:Name="focused" Margin="1,1,1,0" Opacity="0">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFD9F0FC" Offset="0" />
                                            <GradientStop Color="#00D9F0FC" Offset="1" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="holdBorder" Margin="1,1,0,0" Visibility="Collapsed">
                            <Border x:Name="holdBorderNormal" BorderBrush="#FF898C95" BorderThickness="1" Padding="1" Opacity="1">
                                <Border.Background>
                                    <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                        <GradientStop Color="#FFFCFCFC" Offset="0" />
                                        <GradientStop Color="#FFF3F3F3" Offset="1" />
                                        <GradientStop Color="#FFFAFAFA" Offset="0.5" />
                                        <GradientStop Color="#FFF6F6F6" Offset="0.5" />
                                    </LinearGradientBrush>
                                </Border.Background>
                                <Border x:Name="holdInsideBorderNormal">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFF2F2F2" Offset="0" />
                                            <GradientStop Color="#FFCFCFCF" Offset="1" />
                                            <GradientStop Color="#FFEBEBEB" Offset="0.5" />
                                            <GradientStop Color="#FFDDDDDD" Offset="0.5" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                            <Border x:Name="holdBorderSelected" Background="#FFFFFFFF" BorderBrush="#FF898C95" BorderThickness="1" Opacity="0">
                                <Border x:Name="holdBorderFocused" Margin="1,1,1,0" Opacity="0">
                                    <Border.Background>
                                        <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                            <GradientStop Color="#FFD9F0FC" Offset="0" />
                                            <GradientStop Color="#00D9F0FC" Offset="1" />
                                        </LinearGradientBrush>
                                    </Border.Background>
                                </Border>
                            </Border>
                        </Grid>
                        <Grid x:Name="gridContainer">
                            <Grid x:Name="headerContainer" Margin="{TemplateBinding Padding}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="*" />
                                    <ColumnDefinition Width="Auto" />
                                    <ColumnDefinition Width="Auto" />
