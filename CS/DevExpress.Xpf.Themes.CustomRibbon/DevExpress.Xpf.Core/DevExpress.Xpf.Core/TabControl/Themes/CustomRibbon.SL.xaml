                                <dx:DXContentPresenter Grid.Column="2" Content="{TemplateBinding Header}" ContentTemplate="{TemplateBinding HeaderTemplate}" VerticalAlignment="Center" HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" />
                                <Button x:Name="closeButton" Margin="7,1,-4,0" HorizontalAlignment="Right" VerticalAlignment="Center" Style="{StaticResource TabItemThemeKey_CloseButtonStyle}" Grid.Column="4" IsTabStop="False" Visibility="Collapsed" />
                            </Grid>
                        </Grid>
                    </Grid>
                </Grid>
            </dx:HeaderContainerPresenter>
        </Grid>
    </ControlTemplate>

    <Thickness x:Key="TabItemThemeKey_Padding">9,3,9,3</Thickness>

    <sys:Boolean x:Key="TabItemThemeKey_UseLayoutRounding">True</sys:Boolean>

