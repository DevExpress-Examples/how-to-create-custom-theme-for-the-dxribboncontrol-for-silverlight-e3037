    <Style x:Key="TabControlHeaderMenuItemDefaultStyle" TargetType="dx:TabControlHeaderMenuItem">
        <Setter Property="Template" Value="{StaticResource TabControlHeaderMenuThemeKey_TabControlHeaderMenuItemTemplate}" />
    </Style>
    <Style x:Key="TabControlHeaderMenuSeparatorDefaultStyle" TargetType="dx:TabControlHeaderMenuSeparator">
        <Setter Property="Template" Value="{StaticResource TabControlHeaderMenuThemeKey_TabControlHeaderMenuSeparatorTemplate}" />
    </Style>
    <Style x:Key="TabControlHeaderMenuDefaultStyle" TargetType="dx:TabControlHeaderMenu">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="dx:TabControlHeaderMenu">
                    <ContentControl Template="{StaticResource TabControlHeaderMenuThemeKey_TabControlHeaderMenuTemplate}" MaxHeight="400">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto" />
                                <RowDefinition Height="*" />
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <RepeatButton x:Name="PART_ScrollButtonUp" Grid.Row="0" ClickMode="Hover" Template="{StaticResource TabControlHeaderMenuThemeKey_ScrollUpButtonTemplate}" />
                            <ScrollViewer x:Name="PART_Scroll" Grid.Row="1" VerticalScrollBarVisibility="Hidden" HorizontalScrollBarVisibility="Hidden" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0">
                                <ItemsPresenter />
                            </ScrollViewer>
                            <RepeatButton x:Name="PART_ScrollButtonDown" Grid.Row="2" ClickMode="Hover" Template="{StaticResource TabControlHeaderMenuThemeKey_ScrollDownButtonTemplate}" />
                        </Grid>
                    </ContentControl>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <dx:TabControlHeaderMenuPanel />
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
