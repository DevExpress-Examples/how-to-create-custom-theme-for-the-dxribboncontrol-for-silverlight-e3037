                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="normalBorder" Storyboard.TargetProperty="Margin" dxcn:ValueSetter.Thickness="0,-1,0,0" />
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="selected" Storyboard.TargetProperty="BorderThickness" dxcn:ValueSetter.Thickness="1,1,0,1" />
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="selected" Storyboard.TargetProperty="Margin" dxcn:ValueSetter.Thickness="-2,-1,-1,0" />
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="focused" Storyboard.TargetProperty="Background">
